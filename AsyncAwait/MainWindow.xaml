<Window x:Class="AsyncAwait.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AsyncAwait"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock Name="txtBlock" Height="100" ></TextBlock>
            <TextBox Height="100" ></TextBox>
            <Button Height="40" Click="Button_Click" >Start Process</Button>
            <Button Height="40" >Any Button</Button>
        </StackPanel>
    </Grid>
</Window>
